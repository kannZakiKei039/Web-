<?xml version="1.0" encoding="UTF-8"?>
<Context>
	<Resource
		driverClassName="org.postgresql.Driver"
		initialSize="1"
		maxIdle="1"
		maxWaitMillis="-1"
		name="jdbc/mydb2"
		type="javax.sql.DataSource"
		url="jdbc:postgresql://127.0.0.1:5432/mydb2"
		username="postgres"
		password="postgres"
		defaultAutoCommit="false"
	/>
</Context>